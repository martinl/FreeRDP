-
-	if (!settings)
-		return FALSE;
-
-	if (!wlfDisp->activated || !wlfDisp->disp)
-		return TRUE;
-
-	if (GetTickCount64() - wlfDisp->lastSentDate < RESIZE_MIN_DELAY)
-		return TRUE;
-
-	wlfDisp->lastSentDate = GetTickCount64();
-
-	if (!wlf_disp_settings_changed(wlfDisp))
