+	                      ber_sizeof_sequence_octet_string(2 * string_length(csp->ReaderName)) +
+	                      ber_sizeof_sequence_octet_string(2 * string_length(csp->ContainerName)) +
+	                      ber_sizeof_sequence_octet_string(2 * string_length(csp->CspName)));
