+	asn1_null = 5,
+	asn1_object = 6,
+	asn1_object_descriptor = 7,
+	asn1_external = 8,
+	asn1_real = 9,
+	asn1_enumerated = 10,
+	asn1_utf8string = 12,
+	asn1_sequence = 16,
+	asn1_set = 17,
+	asn1_numericstring = 18,
+	asn1_printablestring = 19,
+	asn1_t61string = 20,
+	asn1_teletexstring = asn1_t61string,
+	asn1_videotexstring = 21,
