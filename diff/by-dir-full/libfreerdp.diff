+#define CHECK_MEMORY(pointer, error_action)                                \
+	do                                                                     \
+	{                                                                      \
+		if (!(pointer))                                                    \
+		{                                                                  \
+			WLog_ERR(TAG, "%s:%d: out of memory", __FUNCTION__, __LINE__); \
+			error_action;                                                  \
+		}                                                                  \
+	} while (0)
