+size_t stream_size(size_t allocated_size, writer_pr writer, void* data)
+{
+	size_t written_size;
+	size_t result_size;
+	wStream* s;
+	s = Stream_New(NULL, /* some margin: */ 1024 + 2 * allocated_size);
+	written_size = writer(data, s);
+	Stream_SealLength(s);
+	result_size = Stream_Length(s);
+	Stream_Free(s, TRUE);
+	return result_size;
